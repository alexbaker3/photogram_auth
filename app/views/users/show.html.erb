<div class="page-header">
  <h3>User #<%= @user.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Username</dt>
      <dd><%= @user.username %></dd>

      <dt>Email</dt>
      <dd><%= @user.email %></dd>

      <ul class="list-group">
        <ul>
            <% @user.photos.each do |photo| %>
            <li><%= @user.photos %></li>
            <% end %>
            </ul>
      </ul>

    </dl>


  </div>
</div>
